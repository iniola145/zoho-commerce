            }
        }
        $('.zwc-suites-flex').append($suitesdPrdHtml);
  
        // Related product 
  
        let $relatedPrdHtml = '';
        let $homeRelatedLinks = '';
        if((customvar.productName == 'home')){
            const customersPageOtherLang = ['es-xl', 'de', 'fr', 'nl', 'vi', 'zh-hans'];
            const blogPageOtherLang = ['es-xl', 'pt-br', 'fr', 'th', 'ar', 'id', 'vi'];
            const eventsPageOtherLang = ['th', 'id', 'vi'];
            let zwcNewsletterLink = '';
            let zwcEnterpriseLink  = '';
            if(currentUrlLang == 'th' || currentUrlLang == 'id'){
                zwcNewsletterLink = '<li><a href="'+curUrlLangValPrefix+'/newsletter.html?src=zGlobalTopMenu" >'+Drupal.t('Newsletter')+'</a></li>';
            }
            
            if(CountryCode == 'US'){
                zwcEnterpriseLink = '<li><a href="/enterprise/?src=zGlobalTopMenu">Enterprise</a></li>';
            }
            if(currentUrlLang == 'fr'){
                zwcEnterpriseLink = '<li><a href="/fr/enterprise/?src=zGlobalTopMenu">ETI-Enterprise</a></li>';
            }
            


            $homeRelatedLinks = zwcEnterpriseLink+'<li><a href="'+(customersPageOtherLang.includes(currentUrlLang) ? curUrlLangValPrefix : '') +'/customers.html?src=zGlobalTopMenu">Customers</a></li><li class="zwc-common-dropdown"><div class="zwc-company-dropdown">'+Drupal.t('Company')+'</div><ul class="zwc-company-lists"><li><a href="/aboutus.html?src=zGlobalTopMenu">'+Drupal.t('About us')+'</a></li><li><a href="'+curUrlLangValPrefix+'/ourstory.html?src=zGlobalTopMenu">'+Drupal.t('Our story')+'</a></li><li><a href="/rural-revival/?src=zGlobalTopMenu">'+Drupal.t('Rural revival')+'</a></li><li class="zwc-separationline">&nbsp;</li><li><a href="/press.html?src=zGlobalTopMenu">'+Drupal.t('Press')+'</a></li><li><a href="'+(eventsPageOtherLang.includes(currentUrlLang) ? curUrlLangValPrefix : '') +'/events.html?src=zGlobalTopMenu">'+Drupal.t('Events')+'</a></li><li class="zwc-separationline">&nbsp;</li><li><a href="/careers/?src=zGlobalTopMenu">'+Drupal.t('Careers')+'</a></li><li><a href="/humans-of-zoho/?src=zGlobalTopMenu">'+Drupal.t('Humans of Zoho')+'</a></li><li><a href="/the-long-game/?src=zGlobalTopMenu">'+Drupal.t('The Long Game')+'</a></li></ul></li><li class="zwc-common-dropdown"><div class="zwc-more-links"><span>&nbsp;</span><span>&nbsp;</span><span>&nbsp;</span></div><ul class="zwc-more-link-list"><li><a href="'+(customersPageOtherLang.includes(currentUrlLang) ? curUrlLangValPrefix : '') +'/customers.html?src=zGlobalTopMenu" >'+Drupal.t('Customers')+'</a></li><li><a href="https://www.zoho.com/blog'+(blogPageOtherLang.includes(currentUrlLang) ? curUrlLangValPrefix : '')+'/?src=zGlobalTopMenu" >'+Drupal.t('Blog')+'</a></li><li><a href="https://community.zoho.com/?src=zGlobalTopMenu" >'+Drupal.t('Community')+'</a></li>'+zwcNewsletterLink+'<li class="zwc-separationline">&nbsp;</li><li><a href="https://www.zoho.com/partners/?src=zGlobalTopMenu" >'+Drupal.t('Partner with us')+'</a></li><li><a href="https://help.zoho.com/portal/en/home/?src=zGlobalTopMenu" target="_blank" rel="noopener">'+Drupal.t('Support')+'</a></li><li><a href="'+curUrlLangValPrefix+'/contactus.html?src=zGlobalTopMenu" >'+Drupal.t('Contact us')+'</a></li></ul></li>';
  
        }
        else{
            
            if(data.allProductDetails[customvar.productName] != undefined){
                const relatedPrdList = data.allProductDetails[customvar.productName].relatedProducts;
                if(relatedPrdList){
                        for(let l in relatedPrdList){
                            const relatedprdDetails = data.allProductDetails[relatedPrdList[l]];
                            const relatedprdDesciption = relatedprdDetails['productDesc'][jsonLangKey];
                            const showOnlyRelatedProduct = relatedprdDetails.showOnlyRegion;
                            (showOnlyRelatedProduct != undefined) ? productVisibleCheck(showOnlyRelatedProduct) : productVisible = true;
                            if((relatedprdDesciption != undefined) && productVisible){
                                $relatedPrdHtml +=  '<li class="zwc-product-'+relatedPrdList[l].replace(/[^\w]/g, "")+'"><a href="'+( relatedprdDesciption == '' ? relatedprdDetails.productUrl : curUrlLangValPrefix+relatedprdDetails.productUrl )+'?src=zGlobalRelatedProducts">'+relatedprdDetails.productName+'</a><p>'+(relatedprdDesciption == '' ? relatedprdDetails['productDesc']['en'] : relatedprdDesciption)+'</p></li>';
                            }
  
                        }
                }
            }
  
        }
  
  
        let zwcMenuName =  otherDetailsContent.allProducts[jsonLangKey];
        if(customvar.productName == 'home'){
            zwcMenuName = otherDetailsContent.products[jsonLangKey];
        }

       $('ul.zwc-relatedPrd-wrap').append($relatedPrdHtml+'<li class="zwc-all-product">'+zwcMenuName+'</li>'+$homeRelatedLinks);
  
    // Related product tooltip script
  
    const tooltips = document.querySelectorAll('.zgh-nav > ul > li > p');
    window.onmousemove = function(e) {
        let x = (e.clientX + 0) + 'px',
            y = (e.clientY + 17) + 'px';
        for (let i = 0; i < tooltips.length; i++) {
            if ($(tooltips[i]).parent().is(":hover")) {
                tooltips[i].style.top = y;
                tooltips[i].style.left = x;
            } else {
                tooltips[i].style.top = "0";
                tooltips[i].style.left = "0";
            }
        }
    };

    if(customvar.zohouser){
        customvar.globalHeaderLogged();
      }
  }
  
  
  // Logged User's DC based product hide
  customvar.globalHeaderLogged = function(){
    Object.entries(customvar.dcObj).forEach(([key,value]) => {
        if(dcdomainOne == value.domain){
            if(value.notavailable){
                for(let i = 0; i < value.notavailable.length; i++){
                    $('.zw-global-header .zwc-product-'+(value.notavailable[i].replace(/[^\w]/g, ""))).remove();
                    if(value.notavailable[i] == 'analytics'){
                        $('.zw-global-header .zwc-product-embeddedbi').remove();
                    }
                }
               
            }
        }
    });
    
    for(let i = 0; i < $('.zwc-product-details').length; i++){
        const prdVisibleLength = $('.zwc-product-details').eq(i).find('.zwc-product-list').length;
        const removeClassName = $('.zwc-product-details').eq(i).attr('class').split(' ')[1];
        if(prdVisibleLength == 0){
            $('.zwc-product-categories ul li[data-class="'+removeClassName+'"]').remove();
        }
    }
  }

  
  $(document).ready(function(){

    const zwcPromoBannerHeight =  $('header .zw-promo-top').height();
    let zwcBannerLength = '';
    if(zwcPromoBannerHeight > 0){
        zwcBannerLength = 1;
    }
    if(zwcPromoBannerHeight > 70){
        zwcBannerLength = 2;
    }
    if(zwcPromoBannerHeight > 120){
        zwcBannerLength = 3;
    }
    if(zwcBannerLength){
        $('body').addClass('zwc-banner-length-'+zwcBannerLength);
    }
  
    $(document).on('click', '.zwc-all-product', function() {
        if(!$(this).hasClass('active')){
            $(this).addClass('active');
            $('.zwc-global-menu-container').addClass('active');
            $('.zwc-product-categories li').eq(0).trigger('click');
            $('body').addClass('zwc-body-hidden')
            $('main').append('<div class="zwc-globalmenu-open"></div>');
            searchHide();
        }
        else{
            zwcGlobalMenuClose();
        }
        $('.zwc-common-dropdown').removeClass('active');
    });
    $(document).on('click', '.zwc-gh-close-btn,.zwc-globalmenu-open', function() {
        zwcGlobalMenuClose();
    });
   
    $(document).on('click', '.zwc-product-categories ul li', function() {
        $('.zwc-product-categories ul li').removeClass('active')
        $(this).addClass('active');
        $('.zwc-product-details').removeClass('active');
        $('.zwc-product-details.'+$(this).attr('data-class')).addClass('active');
    });
    
    $(document).on('click', '.zwc-tab-category li', function() {
        if( $(this).find('a').length == 0){
            $('.zwc-tab-category li').removeClass('active');
            $('.zwc-tab-category-list').removeClass('active');
            $(this).addClass('active');
            $('.zwc-tab-category-list').eq($(this).index()).addClass('active');
        }
        
    });
  
    $(document).on(selectEvents, 'body', function(event) {
        event.stopPropagation();
        if (!$('.zwc-common-dropdown').is(event.target) && $(event.target).parents('.zwc-common-dropdown').length === 0 ) {
            $('.zwc-common-dropdown').removeClass('active');
        }
  
    });
    $(document).on('click', '.zwc-common-dropdown', function(e) {
        zwcGlobalMenuClose();
        if($(this).hasClass('active') && !$('.zwc-common-dropdown ul').is(e.target) && $('.zwc-common-dropdown ul').has(e.target).length === 0){
            $(this).removeClass('active');
        }
        else{
            $('.zwc-common-dropdown').removeClass('active');
            $(this).addClass('active');
           
        }
    });
  
  
  
        /*  Search Starts */
        $(document).on('click', '.zgh-search-icon', function() {
            
            if ($('.zgh-search').hasClass('active')) {
                $('.zgh-search').removeClass('active');
                $('.zgh-search-field').blur().val('');
            }
            else{
                $('.zgh-search').addClass('active');
                $('.zgh-search-field').focus().val('');
            }
            
        });
  
        function searchHide() {
            if ($('.zgh-search').hasClass('active')) {
                $('.zgh-search').removeClass('active');
                $('.zgh-search-field').blur().val('');
            }
        }
        $(document).on(selectEvents, '.zgh-search-clear,.zgh-search-overlay', function() {
            searchHide();
        });
        $(document).on('click', '.zgh-search-btn', function() {
            $('#zgh-search-action').submit();
        });
        $(document).on('submit', '#zgh-search-action', function(e) {
            e.preventDefault();
            var formval = $('#zgh-search-query').val().trim();
            var actionURL
           if (currentUrlLang == "") {
                    if(customvar.outproduct.indexOf(customvar.productName) > -1){
                         actionURL = 'https://www'+'.zoho'+'.com/search-results.html';
                    }else{
                        actionURL = window.location.origin + '/search-results.html';
                    }
  
                    if(customvar.productName == 'trainercentral'){
                        actionURL = window.location.origin + '/search-results.html';
                    }
  
                } else {
                    actionURL = window.location.origin + '/' + currentUrlLang + '/' + 'search-results.html';
                }
  
                
  
  
            if (formval != "") {
                window.location = actionURL + "?query=" + encodeURIComponent(formval);
            }
  
            
        });
        $(document).keyup(function(e) {
            if ((e.keyCode === 27)) {
                searchHide();
            }
        });
        $(window).scroll(function() {
            var thisScroll = $(this).scrollTop();
            if (thisScroll > 100) {
                searchHide();
                if((customvar.productName == 'home')){
                    $('.zwc-common-dropdown').removeClass('active');
                }
            }
        });
        /*  Search Ends */
        /*  Localization Starts */
        $(document).on('mouseenter touchstart', '.zgh-localization', function() {
            var that = $(this);
            that.addClass('active');
        });
        $(document).on('mouseleave', '.zgh-localization', function() {
            var that = $(this);
            that.removeClass('active');
        });
        if (typeof detectedLangNew != 'undefined') {
            var _incount = 0;
            var intervar = setInterval(function() {
                if (detectedLangNew.length > 0) {
                    clearInterval(intervar);
                    $('.zgh-localization').addClass('init');
                } else if (_incount > 50) {
                    clearInterval(intervar);
                }
                _incount++;
            }, 1000);
        }
        /*  Localization Ends */
  
    /*  Account Starts*/
    $(document).on(selectEvents, '.zgh-userAvatar', function() {
        var that = $(this);
        that.parent().toggleClass('active');
    });
    /*  Account Ends */
  
  
    $(document).on(selectEvents, 'body', function(event) {
        event.stopPropagation();
        var container = $('.zgh-userPanel');
        if (!container.is(event.target) && container.has(event.target).length === 0) {
            container.removeClass('active');
        }
    });
    $(document).on(selectEvents, 'body', function(event) {
        event.stopPropagation();
        var container = $('.zgh-localization');
        if (!container.is(event.target) && container.has(event.target).length === 0) {
            container.removeClass('active');
        }
    });
  
  });