    }
    if(!zwcRemoveGlobalHeaderProduct.includes(customvar.productName)){
        if(!zwcRemoveGlobalHeaderLang.includes(currentUrlLang)){
            $.getJSON(getJsonFile, function(data) {
                globalMenuData(data.globalMenuData);
